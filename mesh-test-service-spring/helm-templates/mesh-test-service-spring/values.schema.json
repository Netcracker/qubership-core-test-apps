{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The test-service-spring schema",
  "description": "The JSON schema for test-service-spring.",
  "default": {},
  "examples": [
    {
      "PAAS_PLATFORM": "KUBERNETES",
      "ALLOWED_HEADERS": "",
      "API_DBAAS_ADDRESS": "http://dbaas-aggregator.dbaas:8080",
      "CERTIFICATE_BUNDLE_MD5SUM": "d41d8cd98f00b204e9800998ecf8427e",
      "CLIENT_SECRET": "/etc/secret",
      "CPU_REQUEST": "100m",
      "CPU_LIMIT": "4",
      "CUSTOM_HOST": "",
      "GLOWROOT_CLUSTER": "glowroot:8181",
      "IMAGE_REPOSITORY": "",
      "MEMORY_LIMIT": "808Mi",
      "NAMESPACE": "",
      "OPENSHIFT_SERVER": "",
      "PRIVATE_GATEWAY_URL": "",
      "PUBLIC_GATEWAY_URL": "",
      "RC_REVISIONS": 3,
      "REPLICAS": 1,
      "SERVER_HOSTNAME": "",
      "SSL_SECRET": "defaultsslcertificate",
      "TAG": "",
      "XNIO_IO_THREAD": 14,
      "XNIO_WORK_THREAD": 112,
      "X_JAVA_ARGS": "",
      "ARTIFACT_DESCRIPTOR_VERSION": "",
      "MONITORING_INSTALL": false,
      "CLOUD_TOPOLOGY_KEY": "kubernetes.io/hostname"
    }
  ],
  "properties": {
    "ALLOWED_HEADERS": {
      "$id": "#/properties/ALLOWED_HEADERS",
      "type": "string",
      "title": "The ALLOWED_HEADERS schema",
      "description": "headers allowed for forwarding.",
      "default": "",
      "examples": [
        "x-request-id,acceptlanguage"
      ]
    },
    "CERTIFICATE_BUNDLE_MD5SUM": {
      "$id": "#/properties/CERTIFICATE_BUNDLE_MD5SUM",
      "type": "string",
      "title": "The CERTIFICATE_BUNDLE_MD5SUM schema",
      "examples": [
        "d41d8cd98f00b204e9800998ecf8427e"
      ],
      "internal": true
    },
    "CLIENT_SECRET": {
      "$id": "#/properties/CLIENT_SECRET",
      "type": "string",
      "title": "The CLIENT_SECRET schema",
      "description": "Client secret volume path.",
      "examples": [
        "/etc/secret"
      ],
      "internal": true
    },
    "CPU_REQUEST": {
      "$id": "#/properties/CPU_REQUEST",
      "type": "string",
      "title": "The CPU_REQUEST schema",
      "internal": true
    },
    "CUSTOM_HOST": {
      "$id": "#/properties/CUSTOM_HOST",
      "type": "string",
      "title": "The CUSTOM_HOST schema",
      "description": "Openshift custom host name.",
      "default": "",
      "internal": true
    },
    "GLOWROOT_CLUSTER": {
      "$id": "#/properties/GLOWROOT_CLUSTER",
      "type": "string",
      "title": "The GLOWROOT_CLUSTER schema",
      "description": "Host:Port for central glow root.",
      "examples": [
        "glowroot:8181"
      ]
    },
    "IMAGE_REPOSITORY": {
      "$id": "#/properties/IMAGE_REPOSITORY",
      "type": "string",
      "title": "The IMAGE_REPOSITORY schema",
      "default": "",
      "internal": true
    },
    "NAMESPACE": {
      "$id": "#/properties/NAMESPACE",
      "type": "string",
      "title": "The NAMESPACE schema",
      "description": "The namespace it is going to be deployed.",
      "default": "",
      "internal": true
    },
    "PRIVATE_GATEWAY_URL": {
      "$id": "#/properties/PRIVATE_GATEWAY_URL",
      "type": "string",
      "title": "The PRIVATE_GATEWAY_URL schema",
      "description": "Private gateway url.",
      "default": ""
    },
    "PUBLIC_GATEWAY_URL": {
      "$id": "#/properties/PUBLIC_GATEWAY_URL",
      "type": "string",
      "title": "The PUBLIC_GATEWAY_URL schema",
      "description": "Public gateway url.",
      "default": ""
    },
    "RC_REVISIONS": {
      "$id": "#/properties/RC_REVISIONS",
      "type": "integer",
      "title": "The RC_REVISIONS schema",
      "description": "revisionHistoryLimit value for DC.",
      "internal": true
    },
    "REPLICAS": {
      "$id": "#/properties/REPLICAS",
      "type": "integer",
      "title": "The REPLICAS schema",
      "internal": true
    },
    "SSL_SECRET": {
      "$id": "#/properties/SSL_SECRET",
      "type": "string",
      "title": "The SSL_SECRET schema",
      "description": "SSL secret name.",
      "examples": [
        "defaultsslcertificate"
      ],
      "internal": true
    },
    "TAG": {
      "$id": "#/properties/TAG",
      "type": "string",
      "title": "The TAG schema",
      "default": "",
      "internal": true
    },
    "XNIO_IO_THREAD": {
      "$id": "#/properties/XNIO_IO_THREAD",
      "type": "integer",
      "title": "The XNIO_IO_THREAD schema",
      "description": "Number of IO threads in Undertow server.",
      "internal": true
    },
    "XNIO_WORK_THREAD": {
      "$id": "#/properties/XNIO_WORK_THREAD",
      "type": "integer",
      "title": "The XNIO_WORK_THREAD schema",
      "description": "umber of working threads in Undertow server.",
      "internal": true
    },
    "X_JAVA_ARGS": {
      "$id": "#/properties/X_JAVA_ARGS",
      "type": "string",
      "title": "The X_JAVA_ARGS schema",
      "description": "Additional params to pass to Java process",
      "default": "",
      "internal": true
    },
    "ARTIFACT_DESCRIPTOR_VERSION": {
      "$id": "#/properties/ARTIFACT_DESCRIPTOR_VERSION",
      "type": "string",
      "title": "The ARTIFACT_DESCRIPTOR_VERSION schema",
      "description": "Artifact descriptor version which is installed.",
      "default": "",
      "internal": true
    },
    "MONITORING_INSTALL": {
      "$id": "#/properties/MONITORING_INSTALL",
      "type": "boolean",
      "title": "The MONITORING_INSTALL schema",
      "description": "Flag to install dashboard and podmonitor.",
      "examples": [
        false
      ]
    },
    "CLOUD_TOPOLOGY_KEY": {
      "$id": "#/properties/CLOUD_TOPOLOGY_KEY",
      "type": "string",
      "title": "The CLOUD_TOPOLOGY_KEY schema",
      "description": "Affinity key.",
      "examples": [
        "kubernetes.io/hostname"
      ]
    },
    "TLS_SECRET": {
      "$id": "#/properties/TLS_SECRET",
      "type": "string",
      "title": "The TLS_SECRET schema",
      "description": "TLS secret volume path.",
      "examples": [
        "/etc/tls"
      ],
      "internal": true
    },
    "INTERNAL_TLS_ENABLED": {
      "$id": "#/properties/INTERNAL_TLS_ENABLED",
      "type": "boolean",
      "title": "The INTERNAL_TLS_ENABLED schema",
      "description": "Is TLS enabled",
      "examples": [
        false
      ],
      "internal": true
    },
    "TLS_PASSWORD_SECRET_NAME": {
      "$id": "#/properties/TLS_PASSWORD_SECRET_NAME",
      "type": "string",
      "title": "The TLS_PASSWORD_SECRET_NAME schema",
      "description": "TLS password secret name",
      "examples": [
        "tls-password-secret"
      ],
      "internal": true
    },
    "TLS_PASSWORD_KEY": {
      "$id": "#/properties/TLS_PASSWORD_KEY",
      "type": "string",
      "title": "The TLS_PASSWORD_KEY schema",
      "description": "TLS password key",
      "examples": [
        "tls-password"
      ],
      "internal": true
    }
  },
  "additionalProperties": true
}
